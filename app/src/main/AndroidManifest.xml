<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.luwei.itemdecorationdemo">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".linear.SimpleDividerActivity" />
        <activity android:name=".linear.CustomDividerActivity" />
        <activity android:name=".grid.TableDividerActivity" />
        <activity android:name=".stick.StickHeaderActivity" />
        <activity android:name=".staggered.StaggeredActivity" />
        <activity android:name=".grid.GridSpaceActivity" />
    </application>

</manifest>